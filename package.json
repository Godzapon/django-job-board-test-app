{
  "name": "django-job-board",
  "version": "1.0.0",
  "description": "Django Job Board with Tailwind CSS",
  "scripts": {
    "build:css": "tailwindcss -i ./static/src/input.css -o ./static/dist/output.css --minify",
    "watch:css": "tailwindcss -i ./static/src/input.css -o ./static/dist/output.css --watch",
    "dev": "npm run watch:css"
  },
  "keywords": ["django", "tailwind", "job-board"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "tailwindcss": "^3.4.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10"
  }
}

